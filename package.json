    "vuex": "^3.1.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@intlify/eslint-plugin-vue-i18n": "^0.4.0",
